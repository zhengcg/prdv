/* pages/me/me.wxss */
.head{
  background: #EAF090;
  height: 200rpx;
  position: relative;
}
.imgBox{
  width: 128rpx;
  height: 128rpx;
  background: #fff;
  border-radius: 50%;
  overflow: hidden;
  left: 50rpx;
  top: 36rpx;
  position: absolute;
}
.imgBox image{
  width: 128rpx;
  height: 128rpx;
  display: block;
  margin: 0 auto;
}
.nickBox{
  position: absolute;
  left: 204rpx;
  height: 98rpx;
  top: 51rpx;
}
.nickBox view{
  height: 50rpx;
  line-height: 50rpx;
  color: #111;
  font-size: 36rpx;
  margin-bottom: 8rpx;
}
.nickBox text{
  display: block;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  color: #111111;
  padding-right: 30rpx;
}
.head button{
  position: absolute;
  right: 38rpx;
  background: #FFFFFF;
  border: 2rpx solid rgba(244,166,0,0.29);
  border-radius: 32rpx;
  width: 132rpx;
  height: 64rpx;
  line-height: 64rpx;
  top: 68rpx;
  font-size: 28rpx;
  color: #F39800;
}
.head button::after{
  border: 0;
}
.navBox{
  height: 172rpx;
  display: flex;
  box-sizing: border-box;
}
.navBox navigator{
  flex-grow:1;
  text-align: center;
  padding-top: 32rpx;
  font-size: 32rpx;
  color: #111111;
}
.navBox navigator image{
  width: 50rpx;
  height: 50rpx;
  display: block;
  margin-left: auto;
  margin-right: auto;
  margin-bottom: 12rpx;
}
.list{
  padding: 0 40rpx;
}
.list navigator{
  height: 100rpx;
  line-height: 100rpx;
  color: #111;
  font-size: 32rpx;
  border-bottom: 1rpx solid #E5E5E5;
  background: url("data:image/png;base64,") no-repeat right center;
  background-size: 24rpx 24rpx;

}