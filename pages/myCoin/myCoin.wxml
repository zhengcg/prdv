<view class="bg"></view>
<view class='head'>
  <view><image src='../../images/icon16.png'/><text>{{main}}</text></view>
</view>
<view class='nav'>
  <navigator url='/pages/getCoinList/getCoinList'><image src='../../images/icon17.png'/><text>汪汪币记录</text></navigator>
  <navigator url='/pages/exchangeList/exchangeList'><image src='../../images/icon18.png'/><text>汪汪币兑换</text></navigator>
</view>
<view class='taskList'>
  <view class='taskHead'>每日任务</view>
  <navigator class='task' wx:for="{{list}}" wx:key="{{item.task_id}}">
    <view class='text'>{{item.remarks}}</view>
    <view class='coinNum'><image src='../../images/icon16.png'/><text>{{item.score}}</text></view>
  </navigator>
</view>
