
.head{
  background: #EAF090;
  height: 228rpx;
  position: relative;
}
.headImg{
  position: absolute;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  border: 4rpx solid rgba(255,255,255,0.70);
  box-sizing: border-box;
  left: 50%;
  top: 50%;
  margin-left: -70rpx;
  margin-top: -70rpx;
  overflow: hidden;
  z-index: 100;
    background: #EAF090;
}
.headImg image{
  display: block;
  width: 136rpx;
  height: 136rpx;
}
.nickBox{
  width: 280rpx;
  height: 180rpx;
  background: #FFFFFF;
  box-shadow: 0 0 4rpx 0 #DFEC2A;
  position: absolute;
  left: 50%;
  margin-left: -140rpx;
  top: 120rpx;
  text-align: center;
  font-size: 32rpx;
color: #F39800;
padding-top: 100rpx;
box-sizing: border-box;
z-index: 10;
}
.tabNav{
  margin-top: 4rpx;
  height: 84rpx;
  line-height: 84rpx;
  display: flex;
  font-size: 28rpx;
  color: #666666;
}
.tabNav view{
  flex-grow:1;
  text-align: center; 
}
.tabNav view.cur{
  color: #18BAFF;

}
.tabNav view text{
  display: block;
  width: 88rpx;
  height: 4rpx;
  margin: 0 auto;
  background: #fff;

}
.tabNav view.cur text{ 
  background: #18BAFF;
}
.logBox{
  padding: 0 40rpx;
}
.logBox .time{
  font-size: 28rpx;
  color: #666666;
  padding: 38rpx 0;
}
.logBox navigator{
  font-size: 32rpx;
  color: #18BAFF;
  line-height: 85rpx;
  border-bottom: 1rpx solid #E5E5E5;
  background: url("data:image/png;base64,") no-repeat right center;
  background-size: 24rpx 24rpx;
}
.logBox navigator:last-of-type{border: none}
.btnBox{
  white-space: nowrap; 
  padding: 42rpx 32rpx;
  height: 80rpx;
}
.btnBox text{
  display: inline-block;
  width: 150rpx;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0 16rpx;
  font-size: 28rpx;
  color: #111111;
  background: #F6F8FC;
  border-radius: 16rpx;
  text-align: center;

}
.btnBox text:first-of-type{
  margin-left: 0;
}
.btnBox text.cur{
  background: #18BAFF;
  color: #fff;
}
.dataList{
  padding: 0 40rpx;
  font-size: 28rpx; 
}
.dataList navigator{
  height: 85rpx;
  line-height: 85rpx;
  text-align: right;
  padding-right: 40rpx;
  color: #FF5B5B;
  border-bottom: 1rpx solid #E5E5E5;
  background: url("data:image/png;base64,") no-repeat right center;
  background-size: 24rpx 24rpx;

}
.dataList navigator text{
  float: left;
  color: #666;
}
.commentBox{
  padding: 40rpx 64rpx;
  font-size: 32rpx;
  color: #111111;
  line-height: 48rpx;
}
