<view class="title">基本信息</view>
<view class='listBox'>
  <view><text>姓名</text>{{info.title.name}} {{info.title.sex}} {{info.title.age}}岁</view>
  <view><text>医院</text>{{info.title.hospital}}</view>
  <view><text>科室</text>{{info.title.keshi}}</view>
  <view><text>检查名称</text>{{info.title.name}}</view>
  <view><text>检查时间</text>{{info.title.check_time}}</view>
</view>
<view style='padding:40rpx;'>
<view id="panel" class="flex-column">
    <view class="flex-cell flex-row">
      <text class="flex-cell flex-row" style='font-size:24rpx;'>项目</text>
      <text class="flex-cell flex-row" style='font-size:24rpx;'>中文名称</text>
      <text class="flex-cell flex-row" style='font-size:24rpx;'>结果</text>
      <text class="flex-cell flex-row" style='font-size:24rpx;'>参考范围</text>
    </view>
    <view class="flex-row flex-cell" wx:for="{{info.info}}" wx:key="{{index}}">
      <text class="flex-cell flex-row">faa</text>
      <text class="flex-cell flex-row">白细胞数目</text>
      <text class="flex-cell flex-row">5.18*109/L</text>
      <text class="flex-cell flex-row">4.00-10.00</text>
    </view>
    
  </view>
</view>



<view style='height:222rpx;'></view>
<view class='footer'>
