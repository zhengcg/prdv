.listBox{
  padding: 0 40rpx;
}
.listBox>view,.listBox>navigator{
  border-bottom: 1rpx solid #E5E5E5;
  height: 90rpx;
  line-height: 90rpx;
  text-align: right;
  font-size: 32rpx;
  color: #111111;
  position: relative;
  
}
.listBox>view.twj{
   background: url("data:image/png;base64,") no-repeat right center;
  background-size: 24rpx 24rpx;
  padding-right: 30rpx;
}
.listBox>view>text,.listBox>navigator>text{
  float: left;
  font-size: 28rpx;
  color: #666666;
  
}
.listBox>view picker{
  position: absolute;
  height: 90rpx;
  top: 0;
  left: 0;
  width: 100%;

}
.listBox>view picker view{
  width: 100%;
  height: 90rpx;
}
.listBox input{
  height: 90rpx;
  line-height: 90rpx;
}
button{
  width: 600rpx;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 44rpx;
  margin: 60rpx auto;
  background: #F39800;
  font-size: 30rpx;
  color: #FFFFFF;
}
button::after{
  border: 0;
}
.sm{
  background: #F6F8FC;
  padding: 40rpx;
  font-size: 28rpx;
  color: #666666;
  line-height: 60rpx;
}
.tit{
  font-size: 32rpx;
  color: #666666;
}