.searchBox{
  position: fixed;
  width: 100%;
  top: 0;
  padding: 14rpx 32rpx;
  box-sizing: border-box;
  background: #F6F8FC;
  z-index: 10000;
}
.searchBox view{
  border-radius: 8rpx;
  height: 72rpx;
  overflow: hidden;
  padding-left: 50rpx;
  box-sizing: border-box;
  background: #fff url("data:image/png;base64,") no-repeat 5rpx center;
  background-size: 40rpx 40rpx;

}
.searchBox view input{
  font-size: 28rpx;
color: #AAAAAA;
height: 72rpx;
line-height: 72rpx;
width: 630rpx;
}


page{
  height: 100%;
}
.city_box{
  height:100%;
  background: #fff;
  display: flex;
}
.city_left{
  flex: 1;
}
.city_right{
  width: 32rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
}
.letter_item{
  flex: 1;
  display: block;
  font-size: 20rpx;
  color: #333;
  text-align: center;
}
.city_locate,.national{
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #efefef;
  font-size: 28rpx;
  color: #333;
  padding-left: 25rpx;
}
.city_locate_title{
  color: #999;
  margin-right: 20rpx;
}
.new_city{
  background: #efefef;
  font-size: 28rpx;
}
.new_city_title{
  line-height: 50rpx;
  color: #999;
  padding-left: 25rpx;
  margin-bottom: 20rpx;
}
.new_city_box{
  display: flex;
  flex-wrap: wrap;
}
.new_city_text{
  width: 200rpx;
  text-align: center;
  line-height: 70rpx;
  background: #fff;
  border-radius: 35rpx;
  margin:0 0 22rpx 22rpx;
}
.city_first_letter{
  line-height: 50rpx;
  height: 50rpx;
  padding-left: 32rpx;
  font-size: 24rpx;
  background: #F8F8F8;
  color: #666;
}
.city_name{
  display: block;
  line-height: 90rpx;
  border-bottom: 1rpx solid #DFDFDF;
  font-size: 32rpx;
  color: #424242;
  padding-left: 40rpx;
}
.addYY{
  position: fixed;
  width: 100%;
  height: 200rpx;
  bottom: 0;
  background: #F39800;
  z-index: 100;
}
.addYY .top{
  padding: 32rpx;
  font-size: 32rpx;
  color: #FFFFFF;
}
.addYY .bot{
  padding: 0 60rpx;
  text-align: right;
}
.addYY .bot text{
  display: inline-block;
  width: 112rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  background: #fff;
  border-radius: 30rpx;
  font-size: 32rpx;
  color: #111111;
  margin-left: 60rpx;
}
.footer{
  position: fixed;
  height: 200rpx;
  bottom: 0rpx;
  width: 100%;
  left: 0;
  background: #fff;
  box-shadow: 0 -2rpx 4rpx 0 rgba(0,0,0,0.12);
}
.footer button{
  width: 600rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #F39800;
  border-radius: 44rpx;
  font-size:30rpx;
  color: #FFFFFF;
  top: 56rpx;
  position: absolute;
  right: 75rpx;
  display: inline-block
}
