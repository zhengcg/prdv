.head{
   background: #EAF090; 
  height: 228rpx;
  position: relative;
}
.headImg{
  position: absolute;
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  border: 4rpx solid rgba(255,255,255,0.70);
  box-sizing: border-box;
  left: 50%;
  top: 50%;
  margin-left: -70rpx;
  margin-top: -80rpx;
  overflow: hidden;
  z-index: 100;
   background: #EAF090; 
}
.headImg image{
  display: block;
  width: 136rpx;
  height: 136rpx;
}
.nickBox{
  width: 90%;
  height: 180rpx;
  background: #FFFFFF;
  box-shadow: 0 0 4rpx 0 #DFEC2A;
  position: absolute;
  left:50%;
  margin-left: -45%;
  top: 70rpx;
  text-align: center;
  font-size: 32rpx;
  color: #3C3C3E;

padding-top: 100rpx;
box-sizing: border-box;
z-index: 10;
}
.addBox{
  width: 200rpx;
  height: 140rpx;
  background: #FFFFFF;
  box-shadow: 0 0 4rpx 0 rgba(35,35,36,0.30);
  position: absolute;
  right: 0;
  bottom: -60rpx;
  text-align: center;
  font-size: 28rpx;
  color: #3C3C3E;
  box-sizing: border-box;
  padding-top: 78rpx
}
.addBtn{
  width: 100rpx;
  height: 100rpx;
  position: absolute;
  border-radius: 50%;
  background: #D2D882;
  border: 2rpx solid rgba(255,255,255,0.60);
  text-align: center;
  line-height: 100rpx;
  right: 50rpx;
  top: 102rpx;
  color: #fff;
  z-index: 100;
  font-size: 50rpx;
}
.listBox{
  padding: 0 40rpx;
  /* margin-top:100rpx; */
}
.listBox>view,.listBox>navigator{
  border-bottom: 1rpx solid #E5E5E5;
  height: 90rpx;
  line-height: 90rpx;
  text-align: right;
  font-size: 32rpx;
  color: #111111;
  background: url("data:image/png;base64,") no-repeat right center;
  background-size: 24rpx 24rpx;
  padding-right: 30rpx;
  position: relative;
}
.listBox>view>text,.listBox>navigator>text{
  float: left;
  font-size: 28rpx;
  color: #666666;
  
}
.listBox>view picker{
  position: absolute;
  height: 90rpx;
  top: 0;
  left: 0;
  width: 100%;

}
.listBox>view picker view{
  width: 100%;
  height: 90rpx;
}
.imgBox{
  padding: 30rpx 86rpx;
  display: flex;
  flex-wrap:wrap;
  justify-content:space-between;
}
.imgBox navigator,.imgBox view{
  margin: 30rpx 0;
  background: #F6F8FC;
  border: 2rpx solid #EBEEF3;
  width: 260rpx;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  font-size: 32rpx;
  color: #111111;
}
.imgBox navigator.sm,.imgBox view.sm{
  line-height: 80rpx;
}
.imgBox navigator.sm text,.imgBox view.sm text{
  display: block;
  font-size: 24rpx;
  color: #AAAAAA;
  line-height: 30rpx;
}