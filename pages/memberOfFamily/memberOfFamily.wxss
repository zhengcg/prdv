.addBtn{
  background: #F6F8FC;
  padding: 40rpx;
}
.addBtn button{
  background: #FFFFFF;
  height: 108rpx;
  line-height: 108rpx;
  font-size: 32rpx;
  color: #3C3C3E;
}
.addBtn button text{
  color: #F39800;
  font-size: 50rpx;
}
.addBtn button::after{
  border: 0
}
.listBox navigator{
  display: flex;
  padding: 0 52rpx;
  height: 204rpx;
  justify-content:space-between;
  align-items:center;
  border-bottom:1rpx solid #E9E9ED;
  font-size: 32rpx;
  color: #222222;
}
.listBox navigator image{
  width: 128rpx;
  height: 128rpx;
  border-radius: 50%;

}
.listBox navigator text{
  display: block;
  width: 24rpx;
  height: 24rpx;
  background: url("data:image/png;base64,") no-repeat right center;
  background-size: 24rpx 24rpx;
}
.listBox navigator view{
  width: 460rpx;
}