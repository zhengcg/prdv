.headBox{
  height: 300rpx;
  position: relative
}
.headBox view{
  width: 200rpx;
  border-radius: 50%;
  position: absolute;
  bottom: 34rpx;
  left: 50%;
  margin-left: -100rpx;
  overflow: hidden;
  background: #ccc;
}
.headBox.y view{
  width: 200rpx;
  height: 200rpx;
  background:#eee url("data:image/png;base64,") no-repeat center;
  background-size: contain;
  border-radius: 50%;
  position: absolute;
  bottom: 34rpx;
  left: 50%;
  margin-left: -100rpx;
  overflow: hidden
}
.headBox view image{
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto
}
.title{
  padding: 0 40rpx;
  font-size: 28rpx;
  color: #666666;
  height: 80rpx;
  line-height: 80rpx;
}
.listBox{
  padding: 0 40rpx;
}
.listBox>view,.listBox>navigator{
  border-bottom: 1rpx solid #E5E5E5;
  height: 90rpx;
  line-height: 90rpx;
  text-align: right;
  font-size: 32rpx;
  color: #111111;
  background: url("data:image/png;base64,") no-repeat right center;
  background-size: 24rpx 24rpx;
  padding-right: 30rpx;
  position: relative;
}
.listBox>view>text,.listBox>navigator>text{
  float: left;
  font-size: 28rpx;
  color: #666666;
  
}
.listBox>view picker{
  position: absolute;
  height: 90rpx;
  top: 0;
  left: 0;
  width: 100%;

}
.listBox>view picker view{
  width: 100%;
  height: 90rpx;
}
.listBox>view input{
  height: 90rpx;
  line-height: 90rpx;
}

.footer{
  position: fixed;
  height: 200rpx;
  bottom: 22rpx;
  width: 100%;
  left: 0;
  box-shadow: 0 -2rpx 4rpx 0 rgba(0,0,0,0.12);
}
.footer button{
  width: 600rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #F39800;
  border-radius: 44rpx;
  font-size:30rpx;
  color: #FFFFFF;
  top: 56rpx;
  position: absolute;
  left: 75rpx;
}
.footer button::after{border: 0;}