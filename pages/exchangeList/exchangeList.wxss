.tit{
  padding: 20rpx 0;
}
.tit view{
  border-left: 8rpx solid #ffa626;
  padding-left: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  font-size: 12px;
  color: #666666;
}
.listBox{
  border-top: 1rpx solid  #E5E5E5;
  display: flex;
  flex-wrap:wrap;
}
.listBox navigator{
  width: 50%;
  box-sizing: border-box;
  border-bottom: 1rpx solid  #E5E5E5;
  text-align: center;
  padding: 30rpx;

}
.listBox navigator:nth-child(2n+1){
  border-right: 1rpx solid  #E5E5E5;
}
.listBox navigator .imgBox{
  height: 168rpx;
  margin: 0 auto;
  text-align: center;
  margin-bottom: 20rpx;
}
.listBox navigator .imgBox image{
  display: block;
  margin: 0 auto;
  height: 168rpx;
  width: 100%;
}
.listBox navigator .t{
  font-size: 32rpx;
  color: #111111;
  margin-bottom: 20rpx;
}
.listBox navigator .n{
  font-size: 28rpx;
  color: #FF5B5B;
}