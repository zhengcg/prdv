.clearfix::after{
  display: block;
  content: '';
  height: 0;
  clear: both;
}
#panel{
  border-left: 1rpx solid #EAEDF3;
  border-top: 1rpx solid #EAEDF3;
}
#panel text{
  line-height: 90rpx;
  border-right: 1rpx solid  #EAEDF3;
  border-bottom: 1rpx solid  #EAEDF3;
  font-size: 28rpx;
color: #111111;
}

.flex-row{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.flex-column{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: stretch;
}

.flex-cell{
  flex: 1;
}
.pickers{
  position: absolute;
  z-index: 1000;
  opacity: 0.5;
  width: 280rpx;
  height: 250rpx;
  left: 50%;
  margin-left: -140rpx;
  top: 50rpx;
  overflow: hidden;
}
.pickers view{
   width: 280rpx;
  height: 250rpx;
}

.closeBtn{
  display: block;
  position: absolute;
  opacity: 0.3;
  background: #000000 url("data:image/png;base64,") no-repeat center;
  background-size: 25rpx 25rpx;
  width: 40rpx;
  height: 40rpx;
  right: 0;
  bottom: 0
}
.uploadBtn{
  box-sizing: border-box;
  background: #F6F8FC url("data:image/png;base64,") no-repeat center;
  background-size: 56rpx 56rpx;
  border: 2rpx dotted #E5E5E5;
}

.rongqi{
  background: #EAF090;
  height: 228rpx;
  position: relative;
}
.rongqi .item{
  float: left;
  /* border:2rpx solid blue; */
  height:200rpx;
  width:200rpx;
  position:absolute;
  left:-200rpx;
  top:50rpx;
}
.rongqi .item.itemcenter{
  float: left;
  /* border:2rpx solid yellow; */
  height:350rpx;
  width:350rpx;
  top:40rpx;
}
.rongqi .item.itemcenter .headImg{
  margin-top: -160rpx;
  
}
.rongqi .item.itemcenter .nickBox{
  height: 200rpx;
  padding-top: 120rpx;
  color: #F39800;
}
.rongqi .item.itemleft1{
  left:-200rpx;
}
.rongqi .item.itemleft2{
   transition: all 0.5s ease-in; 
  left:-200rpx;
}
.rongqi .item.itemleft3{
  transition: all 0.5s ease-in;
  left:0rpx;
}
.rongqi .item.itemcenter{
  transition: all 0.5s ease-in;
  left:200rpx;
}
.rongqi .item.itemright2{
  transition: all 0.5s ease-in;
  left:750rpx;
}
.rongqi .item.itemright3{
  transition: all 0.5s ease-in;
  left:550rpx;
}
.rongqi .item.itemright1{
  left:750rpx;
}
.usermotto {
  margin-top: 200px;
}
.swiper{
  overflow: hidden;
  height: 350rpx;
}