.clearfix::after{
  display: block;
  content: '';
  height: 0;
  clear: both;
}
#panel{
  border-left: 1rpx solid #EAEDF3;
  border-top: 1rpx solid #EAEDF3;
}
#panel text{
  line-height: 90rpx;
  border-right: 1rpx solid  #EAEDF3;
  border-bottom: 1rpx solid  #EAEDF3;
  font-size: 28rpx;
color: #111111;

}
#panel input{
  line-height: 90rpx;
  border-right: 1rpx solid  #EAEDF3;
  border-bottom: 1rpx solid  #EAEDF3;
  font-size: 28rpx;
color: #111111;
padding: 0 10rpx;
height: 90rpx;

}
#panel text input{
  width: 100%;
}

.flex-row{
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items:stretch;
}
.flex-column{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: stretch;
}

.flex-cell{
  flex: 1;
}
.pickers{
  position: absolute;
  z-index: 1000;
  opacity: 0.5;
  width: 280rpx;
  height: 250rpx;
  left: 50%;
  margin-left: -140rpx;
  top: 50rpx;
  overflow: hidden;
}
.pickers view{
   width: 280rpx;
  height: 250rpx;
}

.closeBtn{
  display: block;
  position: absolute;
  opacity: 0.3;
  background: #000000 url("data:image/png;base64,") no-repeat center;
  background-size: 25rpx 25rpx;
  width: 40rpx;
  height: 40rpx;
  right: 0;
  bottom: 0
}
.uploadBtn{
  box-sizing: border-box;
  background: #F6F8FC url("data:image/png;base64,") no-repeat center;
  background-size: 56rpx 56rpx;
  border: 2rpx dotted #E5E5E5;
}

.rongqi{
  background: #EAF090;
  height: 228rpx;
  position: relative;
}
.rongqi .item{
  float: left;
  height:110rpx;
  width:200rpx;
  position:absolute;
  left:-200rpx;
  top:148rpx;
  text-align: center;
}
.rongqi .item.itemcenter{
  float: left;
  height:160rpx;
  width:350rpx;
  top:116rpx;
}
.rongqi .headImg{
  width: 76rpx;
  height: 76rpx;
  margin-left: auto;
  margin-right: auto;
  margin-top: -148rpx;
  border-radius: 50%;
  overflow: hidden;
  background: #ccc;
   border: 6rpx solid rgba(255,255,255,0.50);
   opacity: 0.6;

}
.rongqi .headImg image{
  width: 76rpx;
  height: 76rpx;
  
 
}
.rongqi .item.itemcenter .headImg{
  width: 124rpx;
  height: 124rpx;
  margin-left: auto;
  margin-right: auto;
  margin-top: -222rpx;
  opacity: 1;
  
}
.rongqi .item.itemcenter .headImg image{
    width: 124rpx;
    height: 124rpx;

}
.rongqi .item .nickBox{
  border-radius: 16rpx;
  text-align: center;
  box-sizing: border-box;
  margin: 0 auto;
  background: #FFFFFF;
  box-shadow: 0 2rpx 10rpx 0 rgba(167,173,73,0.56);
  font-size: 14px;
  color: #111111;
  height: 110rpx;
  padding-top: 50rpx;

}
.rongqi .item.itemcenter .nickBox{
  height: 160rpx;
  width: 300rpx;
   padding-top: 80rpx; 
  font-size: 32rpx;
  color: #F39800;
  
  
}
.rongqi .item.itemleft1{
  left:-200rpx;
}
.rongqi .item.itemleft2{
   transition: all 0.5s ease-in; 
  left:-200rpx;
}
.rongqi .item.itemleft3{
  transition: all 0.5s ease-in;
  left:0rpx;
}
.rongqi .item.itemcenter{
  transition: all 0.5s ease-in;
  left:200rpx;
}
.rongqi .item.itemright2{
  transition: all 0.5s ease-in;
  left:750rpx;
}
.rongqi .item.itemright3{
  transition: all 0.5s ease-in;
  left:550rpx;
}
.rongqi .item.itemright1{
  left:750rpx;
}
.usermotto {
  margin-top: 200px;
}
.swiper{
  overflow: hidden;
  height: 300rpx;
}

.goBack{
  width: 300rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #F39800;
  border-radius: 44rpx;
  font-size:30rpx;
  color: #FFFFFF;
  top: 56rpx;
  position: absolute;
  left: 65rpx;
  display: inline-block;
  text-align: center;
}



.table{
  width: 690rpx;
  margin: 0 auto;
  border: 1rpx solid #EAEDF3;
  text-align: center;
}
.thead,.tr{
  display: flex;
  align-items:stretch;
}
.table.table3 .th,.table.table3 .td{border-bottom: 1rpx solid #EAEDF3;width: 33.3333333%;line-height: 90rpx;color: #111;font-size: 28rpx;}
.table.table2 .th,.table.table2 .td{border-bottom: 1rpx solid #EAEDF3;width: 50%;line-height: 90rpx;color: #111;font-size: 28rpx;}
.table.table4 .th,.table.table4 .td{border-bottom: 1rpx solid #EAEDF3;width: 25%;line-height: 90rpx;color: #111;font-size: 28rpx;}
.table .td{
  padding: 0 10rpx;
}
.th:nth-child(2){border-left: 1rpx solid #EAEDF3;border-right: 1rpx solid #EAEDF3;}
.td:nth-child(2){border-left: 1rpx solid #EAEDF3;border-right: 1rpx solid #EAEDF3;}
